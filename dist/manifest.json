{
  "manifest_version" : 2,
  "name": "Koala Tab",
  "description": "Travel around the world by simply opening a new tab.",
  "version": "1.1.7",
  "permissions" : [
    "management",
    "bookmarks",
    "storage",
    "https://*.traveltab.herokuapp.com/*"
  ],
  "icons" : { 
    "16": "koala_icon.png",
    "48": "koala_icon.png",
    "128": "koala_icon.png" 
  },
  "chrome_url_overrides" : {
      "newtab": "index.html"
  }
}
